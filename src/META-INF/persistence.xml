<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="database1PU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>database1.Book</class>
    <class>database1.BuyBook</class>
    <class>database1.Employee</class>
    <class>database1.SportsEquipment</class>
    <class>database1.Stationery</class>
    <class>database1.Rent</class>
    <class>database1.Customer</class>
    <class>database1.BuyStationery</class>
    <class>database1.BookRent</class>
    <class>database1.BuySport</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:oracle:thin:@localhost:1521:orcl"/>
      <property name="javax.persistence.jdbc.user" value="myproject"/>
      <property name="javax.persistence.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
      <property name="javax.persistence.jdbc.password" value="654321"/>
    </properties>
  </persistence-unit>
</persistence>
